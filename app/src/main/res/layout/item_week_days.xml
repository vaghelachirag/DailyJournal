<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools">


    <data>
        <variable
            name="position"
            type="Integer" />

        <variable
            name="itemData"
            type="com.app.dailyjounral.model.ModelDataWeek" />

        <variable
            name="holder"
            type="com.app.dailyjounral.adapter.WeekItemViewHolder" />

    </data>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/_3ssp">

    <com.app.dailyjounral.widget.RobotoRegularText
        android:id="@+id/txtWeekDays"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="M"
        android:textSize="@dimen/_12ssp"
        android:layout_gravity="center"
        android:textColor="@color/tip_of_day_gradient_top"
        android:gravity="center"
        tools:ignore="MissingConstraints,SpUsage,UnknownIdInLayout">

    </com.app.dailyjounral.widget.RobotoRegularText>

</androidx.constraintlayout.widget.ConstraintLayout>
</layout>